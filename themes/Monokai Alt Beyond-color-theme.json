	"tokenColors": [
		{
			"scope": [
				"comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#757180",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"comment.block.documentation storage.type.class",
				"punctuation.definition.block.tag",
				"entity.name.type.instance"
			],
			"settings": {
				"foreground": "#D8D8D8",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"string",
				"string.template"
			],
			"settings": {
				"foreground": "#E6DB74"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#AE81FF"
			}
		},
		{
			"scope": [
				"string.embedded.begin",
				"string.embedded.end"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"scope": "string.embedded",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
